{
    "name": "skywalker-labs/impersonate",
    "description": "A powerful Laravel package to authenticate as your users. Easily impersonate other users in your application with a simple trait. Includes features like Blade directives, event handling, multi-guard support, and customizable access controls for advanced user management and debugging.",
    "type": "library",
    "license": "MIT",
    "keywords": [
        "laravel",
        "package",
        "plugin",
        "impersonate",
        "impersonation",
        "user",
        "auth",
        "authentication",
        "login-as",
        "user-switching",
        "developer-tools",
        "testing",
        "debugging",
        "security",
        "skywalker-labs"
    ],
    "homepage": "https://github.com/skywalker-labs/impersonate",
    "support": {
        "issues": "https://github.com/skywalker-labs/impersonate/issues",
        "source": "https://github.com/skywalker-labs/impersonate"
    },
    "funding": [
        {
            "type": "github",
            "url": "https://github.com/sponsors/ermradulsharma"
        }
    ],
    "authors": [
        {
            "name": "Skywalker Labs Team",
            "email": "skywalkerlknw@gmail.com",
            "homepage": "https://skywalker-labs.com/",
            "role": "Developer"
        },
        {
            "name": "Mradul Sharma",
            "email": "skywalkerlknw@gmail.com",
            "homepage": "https://mradulsharma.vercel.app/",
            "role": "Lead Developer"
        }
    ],
    "require": {
        "php": "^7.2 | ^8.0",
        "laravel/framework": "^6.0 | ^7.0 | ^8.0 | ^9.0 | ^10.0 | ^11.0 | ^12.0"
    },
    "require-dev": {
        "mockery/mockery": "^1.3.3",
        "orchestra/testbench": "^4.0 | ^5.0 | ^6.0 | ^7.0 | ^8.0 | ^9.0 | ^10.0",
        "phpunit/phpunit": "^7.5 | ^8.0 | ^9.0 | ^10.0 | ^11.0 | ^12.0"
    },
    "autoload": {
        "psr-4": {
            "Skywalker\\Impersonate\\": "src/"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Skywalker\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Skywalker\\Impersonate\\ImpersonateServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}